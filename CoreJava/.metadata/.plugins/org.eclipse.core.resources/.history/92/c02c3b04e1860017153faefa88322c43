package com.coding;

import java.util.ArrayList;
import java.util.Arrays;

public class DuplicatesinArrayList {

	
	public static void main(String[] args) {
		/*ArrayList<String> alr= new ArrayList<String>();
		alr.add("Ramesh");
		alr.add("Ramesh");
		alr.add("kiran");*/
		String[] alr={"Ramesh","Ramesh","Kiran"};
		
		System.out.println(alr);
		for(int i=0;i<alr.length;i++){
			
			for(int j=i+1;j<alr.length;j++){
				if(alr[i]==(alr[j])){
					alr[j]=alr[i+1];
					j--;
				}
			}
		}
		System.out.println(new ArrayList<>(asList(alr)));
		
	}
}
